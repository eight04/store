{
  "name": "@eight04/store",
  "version": "0.2.1",
  "description": "Reactive store aims to work with collection",
  "author": "eight04 <eight04@gmail.com>",
  "license": "MIT",
  "repository": "eight04/store",
  "keywords": [
    "reactive",
    "store",
    "collection",
    "array",
    "set"
  ],
  "scripts": {
    "test": "eslint . --cache && c8 --all mocha -b",
    "build": "tsc --project tsconfig.build.json",
    "build-doc": "typedoc",
